{
  "name": "pos",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "nodemon": "nodemon lib/express/server.js --watch src/express",
    "transpile-server-files": "babel src/express -d lib/express --watch",
    "livereload": "node livereload.js",
    "make-js": "watchify -v -t babelify src/js/scripts.js -o assets/bundle.js",
    "stylus": "stylus src/stylus/styles.styl -u autoprefixer-stylus -o assets/ -w",
    "dev": "parallelshell 'npm run transpile-server-files' 'npm run nodemon' 'npm run livereload' 'npm run stylus' 'npm run make-js'"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "express": "^4.13.4",
    "jade": "^1.11.0",
    "react": "^15.1.0"
  },
  "devDependencies": {
    "autoprefixer": "^6.3.6",
    "autoprefixer-stylus": "^0.9.2",
    "babel": "^6.5.2",
    "babel-cli": "^6.6.5",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babelify": "^7.3.0",
    "live-reload": "^1.1.0",
    "livereload": "^0.4.1",
    "nodemon": "^1.9.1",
    "parallelshell": "^2.0.0",
    "postcss-cli": "^2.5.1",
    "stylus": "^0.54.2",
    "watchify": "^3.7.0"
  }
